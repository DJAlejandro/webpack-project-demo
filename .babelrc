{
    "presets":[
        ["@babel/preset-env",{
            "modules": false,
            "targets": {
                "browsers":["> 1%", "last 2 versions", "not ie <= 8", "Android >= 4.0"]
            },
            "useBuiltIns":"usage",
            "corejs": 2
        }]
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import",// 异步加载语法编译插件
        "lodash"
    ]
}
